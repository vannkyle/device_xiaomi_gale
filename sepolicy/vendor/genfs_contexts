# Extcon
genfscon sysfs /devices/platform/extcon_usb/extcon/extcon0/cable.0/name u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/extcon_usb/extcon/extcon0/cable.1/name u:object_r:sysfs_extcon:s0

# Fingerprint
genfscon sysfs /devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/clk_enable u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/wakeup_enable u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/irq u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/authen_fd/performance u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/modalias u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/fingerprint u:object_r:sysfs_fingerprint:s0

# Health
genfscon sysfs /devices/platform/mt_charger/power_supply/ac u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/battery/power_supply/battery u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/platform/mt_charger/power_supply/usb u:object_r:sysfs_batteryinfo:s0


# Wakeup (https://lkml.org/lkml/2019/8/6/1275)
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt6358-rtc/wakeup u:object_r:sysfs_wakeup:s0                               
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt635x-auxadc/wakeup u:object_r:sysfs_wakeup:s0                            
genfscon sysfs /devices/platform/10238000.gce/wakeup u:object_r:sysfs_wakeup:s0                     
genfscon sysfs /devices/platform/1100d000.i2c6/i2c-6/6-004e/tcpc/type_c_port0/wakeup u:object_r:sysfs_wakeup:s0                                       
genfscon sysfs /devices/platform/1100d000.i2c6/i2c-6/6-004e/wakeup u:object_r:sysfs_wakeup:s0       
genfscon sysfs /devices/platform/1100d000.i2c6/i2c-6/6-006a/power_supply/sc-charger/wakeup u:object_r:sysfs_wakeup:s0                                 
genfscon sysfs /devices/platform/1100d000.i2c6/i2c-6/6-006a/wakeup u:object_r:sysfs_wakeup:s0       
genfscon sysfs /devices/platform/1100f000.i2c3/i2c-3/3-0028/wakeup u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/15020000.imgsys_config/wakeup u:object_r:sysfs_wakeup:s0
